{
  "$schema": "https://registry.nimbletools.ai/schemas/2025-12-11/nimbletools-server.schema.json",
  "name": "ai.nimbletools/nationalparks",
  "version": "0.1.0",
  "title": "National Parks",
  "description": "National Parks Service API for U.S. park info, alerts, campgrounds, and events",
  "icons": [
    {
      "src": "https://static.nimbletools.ai/icons/nationalparks.png",
      "sizes": [
        "64x64"
      ]
    },
    {
      "src": "https://static.nimbletools.ai/logos/nationalparks.png",
      "sizes": [
        "256x256"
      ]
    }
  ],
  "repository": {
    "url": "https://github.com/NimbleBrainInc/mcp-server-nationalparks",
    "source": "github"
  },
  "websiteUrl": "https://www.nps.gov/subjects/developer/api-documentation.htm",
  "packages": [
    {
      "registryType": "mcpb",
      "identifier": "https://github.com/NimbleBrainInc/mcp-server-nationalparks/releases/download/v0.1.0/mcp-server-nationalparks-v0.1.0-linux-amd64.mcpb",
      "version": "0.1.0",
      "fileSha256": "f8829057c3c5865930769b132021827f9d1cde49b541764d096902ce0003ec16",
      "transport": {
        "type": "streamable-http",
        "url": "http://localhost:8000/mcp"
      },
      "environmentVariables": [
        {
          "name": "NPS_API_KEY",
          "description": "National Park Service API key - get one at https://www.nps.gov/subjects/developer/get-started.htm",
          "isRequired": true,
          "isSecret": true
        },
        {
          "name": "LOG_LEVEL",
          "description": "Logging level (optional, defaults to info)",
          "isRequired": false,
          "default": "info"
        }
      ]
    },
    {
      "registryType": "mcpb",
      "identifier": "https://github.com/NimbleBrainInc/mcp-server-nationalparks/releases/download/v0.1.0/mcp-server-nationalparks-v0.1.0-linux-arm64.mcpb",
      "version": "0.1.0",
      "fileSha256": "e8e77d1111628d2da77fbfb2efea82db90569d64af01912cb4dd77b3ef215ec4",
      "transport": {
        "type": "streamable-http",
        "url": "http://localhost:8000/mcp"
      },
      "environmentVariables": [
        {
          "name": "NPS_API_KEY",
          "description": "National Park Service API key - get one at https://www.nps.gov/subjects/developer/get-started.htm",
          "isRequired": true,
          "isSecret": true
        },
        {
          "name": "LOG_LEVEL",
          "description": "Logging level (optional, defaults to info)",
          "isRequired": false,
          "default": "info"
        }
      ]
    }
  ],
  "_meta": {
    "ai.nimbletools.mcp/v1": {
      "runtime": "node:24",
      "status": "active",
      "repository": {
        "branch": "main"
      },
      "container": {
        "healthCheck": {
          "enabled": true,
          "path": "/health",
          "port": 8000,
          "interval": 10
        },
        "startupProbe": {
          "initialDelaySeconds": 15
        }
      },
      "resources": {
        "limits": {
          "memory": "512Mi",
          "cpu": "1000m"
        },
        "requests": {
          "memory": "256Mi",
          "cpu": "250m"
        }
      },
      "scaling": {
        "enabled": true,
        "minReplicas": 1,
        "maxReplicas": 3,
        "targetCPUUtilizationPercentage": 80
      },
      "deployment": {
        "protocol": "http",
        "port": 8000,
        "mcpPath": "/mcp"
      },
      "capabilities": {
        "tools": true,
        "resources": false,
        "prompts": false
      },
      "display": {
        "category": "data-intelligence",
        "tags": [
          "government-data",
          "parks",
          "events",
          "campgrounds",
          "alerts",
          "requires-api-key"
        ],
        "documentation": {
          "readmeUrl": "https://raw.githubusercontent.com/NimbleBrainInc/mcp-server-nationalparks/main/README.md"
        }
      }
    }
  }
}
