{
  "$schema": "https://registry.nimbletools.ai/schemas/2025-12-11/nimbletools-server.schema.json",
  "name": "ai.nimbletools/deepl",
  "version": "0.1.0",
  "title": "DeepL",
  "description": "DeepL API: professional translation for 30+ languages with document support and glossaries",
  "icons": [
    {
      "src": "https://static.nimbletools.ai/icons/deepl.png",
      "sizes": [
        "64x64"
      ]
    },
    {
      "src": "https://static.nimbletools.ai/logos/deepl.png",
      "sizes": [
        "256x256"
      ]
    }
  ],
  "repository": {
    "url": "https://github.com/NimbleBrainInc/mcp-deepl",
    "source": "github"
  },
  "websiteUrl": "https://www.deepl.com/",
  "packages": [
    {
      "registryType": "mcpb",
      "identifier": "https://github.com/NimbleBrainInc/mcp-deepl/releases/download/v0.1.0/mcp-deepl-v0.1.0-linux-amd64.mcpb",
      "version": "0.1.0",
      "fileSha256": "6410abbebe4b34bb93f662937bb686de9d80a59790ee001f11e2188e959c53f0",
      "transport": {
        "type": "streamable-http",
        "url": "https://mcp.nimbletools.ai/mcp"
      },
      "environmentVariables": [
        {
          "name": "DEEPL_API_KEY",
          "description": "DeepL API key (get from https://www.deepl.com/pro-api)",
          "isRequired": true,
          "isSecret": true,
          "placeholder": "your_deepl_api_key:fx"
        }
      ]
    },
    {
      "registryType": "mcpb",
      "identifier": "https://github.com/NimbleBrainInc/mcp-deepl/releases/download/v0.1.0/mcp-deepl-v0.1.0-linux-arm64.mcpb",
      "version": "0.1.0",
      "fileSha256": "d691e13abdb3de4d34d0408987f3a31f0ad32b46d0022532c7889a8e0ea42cbb",
      "transport": {
        "type": "streamable-http",
        "url": "https://mcp.nimbletools.ai/mcp"
      },
      "environmentVariables": [
        {
          "name": "DEEPL_API_KEY",
          "description": "DeepL API key (get from https://www.deepl.com/pro-api)",
          "isRequired": true,
          "isSecret": true,
          "placeholder": "your_deepl_api_key:fx"
        }
      ]
    }
  ],
  "_meta": {
    "ai.nimbletools.mcp/v1": {
      "runtime": "python:3.14",
      "status": "active",
      "repository": {
        "branch": "main"
      },
      "container": {
        "healthCheck": {
          "path": "/health",
          "port": 8000
        }
      },
      "resources": {
        "limits": {
          "memory": "256Mi",
          "cpu": "100m"
        },
        "requests": {
          "memory": "128Mi",
          "cpu": "50m"
        }
      },
      "deployment": {
        "protocol": "http",
        "port": 8000,
        "mcpPath": "/mcp"
      },
      "capabilities": {
        "tools": true,
        "resources": false,
        "prompts": false
      },
      "display": {
        "category": "communication-collaboration",
        "tags": [
          "deepl",
          "translation",
          "languages",
          "localization",
          "i18n",
          "multilingual",
          "documents",
          "glossary",
          "requires-api-key"
        ],
        "documentation": {
          "readmeUrl": "https://raw.githubusercontent.com/NimbleBrainInc/mcp-deepl/main/README.md"
        }
      }
    }
  }
}
